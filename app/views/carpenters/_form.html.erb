<%= form_for @carpenter do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :company %>
  <%= f.text_field :company %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>

  <%= f.submit %>

<% end %>
